<h1>Packages</h1>
<table class="table">
  <thead>
    <tr>
      <th>Package</th>
      <th>Owner</th>
      <th>Last Updated</th>
    </tr>
  </thead>
  {{#packages}}
    <tr>
      <td><strong><a href="/packages/{{name}}">{{name}}</a></strong></td>
      <td>{{owner.nickname}}</td>
      <td>{{updated}}</td>
    </tr>
  {{/packages}}
</table>
