<h1>{{package.name}}</h1>
<h3>Owned by {{package.owner.nickname}}</h3>
<ul>
  {{#versions}}
    <li>
      {{version}}
      (<a href="{{download_url}}">Download</a>)
    </li>
  {{/versions}}
</ul>
{{#is_owner}}
  <p><a href="/packages/{{package.name}}/versions/new">Add Version</a></p>
{{/is_owner}}
