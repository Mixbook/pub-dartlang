<h1>{{package.name}}</h1>
{{#package.description}}
  <p>{{package.description}}</p>
{{/package.description}}
{{#package.authors_html}}
  <h3>Authors: {{& package.authors_html}}</h3>
{{/package.authors_html}}
<h4>Owner: {{package.owner.nickname}}</h2>
<ul>
  {{#versions}}
    <li>
      {{version}}
      (<a href="{{download_url}}">Download</a>)
    </li>
  {{/versions}}
</ul>
{{#show_versions_link}}
  <p>
    <a href="/packages/{{package.name}}/versions">
      All {{version_count}} versions
    </a>
  </p>
{{/show_versions_link}}
{{#is_owner}}
  <p><a href="/packages/{{package.name}}/versions/new">Add Version</a></p>
{{/is_owner}}
